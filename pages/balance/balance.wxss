.container-g {
  display: flex;
  flex-direction: column;
}
.container-g .top {
  height: 327rpx;
  background-color: #BEBDBD;
  padding-left: 12rpx;
  padding-right: 15rpx;
  opacity: 0.5;
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
}
.box {
  position: relative;
}
.top-content {
  position: absolute;
  left: 64rpx;
  top: 0;
  display: flex;
  flex-direction: column;
}
.top-content-text {
  font-size: 36rpx;
  color: #131313;
  margin-top: 89rpx;
  margin-bottom: 60rpx;
}
.top-content-sum {
  font-size: 60rpx;
  color: #FF4040;
}
.bottom {
  display: flex;
  flex-direction: row;
  justify-content: center;
  margin-top: 55rpx;
}

.bottom view {
  width: 172rpx;
  height: 67rpx;
  text-align: center;
  line-height: 67rpx;
  border-radius: 10rpx;
  box-sizing: border-box;
}
.detail {
  border: 2rpx solid #BEBDBD;
  margin-right: 141rpx;
}
.get {
  background-color: #50BE06;
  color: #fff;
}