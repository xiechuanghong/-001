.goodlist {
  height: 146rpx;
  box-sizing: border-box;
  padding: 36rpx 38rpx 0 94rpx;
  font-size: 26rpx;
  border-bottom: 1rpx solid #ccc;
}
.content image {
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  /* left: -40rpx;
  top: 6rpx; */
  left: -54rpx;
  top: 33rpx;
}
.content-info {
  display: flex;
  position: relative;
  justify-content: space-between;
}
.content-info .name {
  color: #323232;
  font-size: 28rpx;
}
.content-info .time {
  color: #161616;
}
.content-num {
  margin-top: 24rpx;
  color: #616161;
}

.jia {
  position: fixed;
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.isShow {
  text-align: center;
}